{
  "name": "bannerhandler",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "per-env",
    "start:wiwi": "yarn start:client & yarn start:server",
    "start:development": "stmux -- [ [ 'yarn start:client' : 'yarn start:server' ] .. 'yarn test:watch' ]",
    "start:production": "node ./server",
    "start:client": "ng build --watch",
    "start:server": "nodemon ./server -w ./server -w .env",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:watch": "watch \"ng e2e -s false\" src e2e --wait=1",
    "ncu": "ncu -u"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/.test-config/setupJest.ts",
    "bail": true
  },
  "contributors": [
    "Mathieu Breton <mathieu.breton@js-republic.com>",
    "Guillaume Barranco <guillaume.barranco@js-republic.com>"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.3",
    "@angular/cdk": "^5.0.0-rc.2",
    "@angular/common": "^5.0.3",
    "@angular/compiler": "^5.0.3",
    "@angular/core": "^5.0.3",
    "@angular/forms": "^5.0.3",
    "@angular/http": "^5.0.3",
    "@angular/material": "^5.0.0-rc.2",
    "@angular/platform-browser": "^5.0.3",
    "@angular/platform-browser-dynamic": "^5.0.3",
    "@angular/router": "^5.0.3",
    "angular2-moment": "^1.7.0",
    "axios": "^0.17.1",
    "body-parser": "^1.18.0",
    "busboy": "^0.2.14",
    "compression": "^1.7.0",
    "connect-busboy": "^0.0.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "core-js": "^2.4.1",
    "envalid": "^4.1.1",
    "express": "^4.15.4",
    "googleapis": "^25.0.0",
    "moment": "^2.18.1",
    "ng2-file-upload": "^1.2.1",
    "npm-check-updates": "^2.13.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "per-env": "^1.0.2",
    "rxjs": "^5.4.2",
    "s3fs": "^2.5.0",
    "serve-static": "^1.13.1",
    "zone.js": "^0.8.14"
  },
