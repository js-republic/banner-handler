    "@types/jasmine": "~2.8.2",
    "@types/jasminewd2": "~2.0.2",
    "@types/jest": "^22.0.1",
    "@types/node": "~9.4.0",
    "codelyzer": "~4.1.0",
    "dotenv": "^5.0.0",
    "jasmine-core": "~2.9.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^22.1.4",
    "jest-preset-angular": "^5.0.0",
    "nodemon": "^1.12.0",
    "protractor": "~5.3.0",
    "stmux": "^1.4.18",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.0"
  }
